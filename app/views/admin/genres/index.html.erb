<h4>ジャンル追加・一覧</h4>
<p>ジャンル名</p>
  <%= form_with model: @genre, url: admin_genres_path, local:true do |f|%>
  <%= f.text_field :name %>
  <%= f.submit %>
  <% end %>

<table>
  <tr>
    <td>ジャンル</td>
    <td></td>
  </tr>
  <tr>
    <% @genres.each do |genre| %>
    <td><%= genre.name %></td>
    <td><%= link_to "Edit", edit_admin_genre_path(genre.id) %></td>
  </tr>
  <% end %>
</table>
